# e-automate PIP API Credentials
EA_USERNAME=your_username
EA_PASSWORD=your_password
EA_COMPANYID=your_company_id

# PIP endpoint
# PIP_ENDPOINT=https://sfs.rpg.com/pip/PublicAPIService.asmx
PIP_ENDPOINT=http://as06/pip/PublicAPIService.asmx

# Google Custom Search API (Optional - Tier 2)
# Get keys from: https://developers.google.com/custom-search/v1/overview
GOOGLE_CSE_KEY=your_google_api_key
GOOGLE_CX=your_custom_search_engine_id

# OpenAI API (Optional - Tier 3)
# Get key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key

# Feature Flags
# Set to 'true' to enable OpenAI as fallback search method
ENABLE_OPENAI=false

# OpenAI Model Selection (if enabled)
# Options: gpt-4o-mini (cheaper), gpt-4o (better quality), gpt-4-turbo
# Default: gpt-4o-mini
OPENAI_MODEL=gpt-4o-mini